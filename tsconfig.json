{
  "compilerOptions": {
    "target": "es2020",
    "module": "NodeNext",
    "moduleResolution":"nodenext",
    "lib": ["esnext", "dom", "es2017.object"],
    "allowJs": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "*": [".//*"]
    }
  }
}